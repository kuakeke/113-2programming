      "source": [
        "# 排除停用詞（這裡提供一個簡單的停用詞列表，可根據需要擴展）\n",
        "stop_words = set(['讚', '沒', '無', '因為', '的', '是', '了', '我', '也', '在', '和', '就', '有', '他', '她', '你', '可以', '這個', '大家'])\n",
        "filtered_word_counts = {word: count for word, count in word_counts.items() if word not in stop_words}"
      ],
      "metadata": {
        "id": "rSvW8m__a-rv"
      },
      "execution_count": 137,
      "outputs": []
