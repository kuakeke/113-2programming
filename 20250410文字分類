      "cell_type": "code",
      "source": [
        "# 合併這幾欄為一個欄位，用空格隔開\n",
        "df['Columns'] = dicts[columns].fillna('').agg(' '.join, axis=1)\n",
        "\n",
        "df['Chinese_Content'] = df['Columns'].apply(extract_chinese)\n",
        "\n",
        "# 使用 Jieba 斷詞\n",
        "df['Tokenized'] = df['Chinese_Content'].apply(lambda x: list(jieba.cut(x, HMM=True)))\n",
        "\n",
        "# 展平成所有詞語的列表\n",
        "all_words = [word for tokens in df['Tokenized'] for word in tokens if len(word) > 1]"
