        "# 替換中文上午／下午為英文格式\n",
        "df['時間戳記'] = df['時間戳記'].str.replace('上午', 'AM', regex=False)\n",
        "df['時間戳記'] = df['時間戳記'].str.replace('下午', 'PM', regex=False)\n",
        "\n",
        "# 然後再轉換成 datetime\n",
        "df['時間戳記'] = pd.to_datetime(df['時間戳記'], format='%Y/%m/%d %p %I:%M:%S')\n",
        "\n",
        "# 取出日期（不含時間）\n",
        "df['Date'] = df['時間戳記'].dt.date\n"
      ],
